{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/third_party/**",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/linux/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/example/build/**",
                "${workspaceFolder}/third_party/cpp-get-map-version/**",
                "${workspaceFolder}/third_party/cpp-spreadsheet/**",
                "${workspaceFolder}/third_party/cpp-spreadsheet/spreadsheet",
                "${workspaceFolder}/third_party/test_cpp"
            ],
            "defines": [
                "-Wall",
                "-Werror",
                "-DTRACE"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}